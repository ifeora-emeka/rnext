{
  "name": "rnext-server",
  "version": "0.0.44",
  "description": "A server for running Next.js and Vite applications together",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && cd ../../examples/cms && bun run build && cp -r dist ../../packages/rnext-server/dist/cms-build",
    "dev": "bun --watch src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build && npm version patch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/node": "^20.0.0",
    "next": "^15.1.4",
    "express": "^4.21.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "keywords": [
    "next.js",
    "vite",
    "server",
    "express",
    "bun"
  ],
  "author": "Ifeora Emeka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idegin-tech/rnext/rnext-server.git"
  },
  "bugs": {
    "url": "https://github.com/idegin-tech/rnext/rnext-server/issues"
  },
  "homepage": "https://github.com/idegin-tech/renext/rnext-server#readme"
}
